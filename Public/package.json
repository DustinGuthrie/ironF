{
  "name": "iron-fund",
  "version": "1.0.0",
  "description": "teamwork with backend",
  "main": "main.js",
  "dependencies": {
    "backbone": "^1.2.3",
    "bootstrap": "^3.3.5",
    "jquery": "^2.1.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "node-sass": "^3.4.2",
    "serve": "^1.4.0",
    "watchify": "^3.6.1"
  },
  "scripts": {
    "bundle": "./node_modules/.bin/browserify main.js -o bundle.js",
